  border-radius: 15rpx;
  overflow: hidden;
  margin-left: 50rpx;
}
.aq{
  display: flex;
  flex-direction: row;
  font-size: 21px;
  font-weight: 400;
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  margin-top: 10rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  padding-left: 20rpx;
}

.aa{
  display: flex;
  flex-direction: row;
  background-color: #3396fa;
  height: 150rpx;
